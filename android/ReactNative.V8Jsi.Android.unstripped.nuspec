<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>$Id$</id>
    <version>$Version$</version>
    <description>Contains the unstripped (i.e with debug symbols) Android implementation of the V8 JSI wrapper for React Native ($VersionDetails$)</description>
    <authors>Facebook, Google, Microsoft</authors>
    <projectUrl>https://github.com/microsoft/v8-jsi/tree/master/android</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <repository type="git" url="$RepoUri$" commit="$CommitId$" />
  </metadata>
  <files>
    <file src="$nugetroot$\android\*.*" target="" />
    <file src="$nugetroot$\android\lib\**\args.gn" target="lib" />
    <file src="$nugetroot$\android\lib\**\lib.unstripped\**\*.*" target="lib" />
    <file src="$nugetroot$\android\headers\include\**\*.*" target="headers" />
  </files>
</package>
